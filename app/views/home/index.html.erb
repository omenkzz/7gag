<% Post.all.each do |post| %>
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title"><%= post.title %></h3>
      </div>
      <div class="panel-body">
        <%# post.body %> <br />
        <%= image_tag post.meme.url %>
      </div>
      <div class="panel-footer">Panel footer</div>
    </div>

<% end %>

